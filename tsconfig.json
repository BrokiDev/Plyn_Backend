{
  "compilerOptions": {
    "target": "ES2016",
    "module": "CommonJS",
    "moduleResolution": "Node",
    // "rootDir": "./src",
    "allowJs": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strictNullChecks": true
  },
  "include": ["src/**/*","src/helpers/*","src/utils/*"],
  "exclude": ["node_modules"]
}
